{
    "manifest_version": 3,
    "name": "GPT_Assistant",
    "description": "GPT_Assistant",
    "version": "1.0",
    "icons": {
        "16": "icons/icon1.png",
        "48": "icons/icon2.png",
        "128": "icons/icon3.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon1.png",
            "48": "icons/icon2.png",
            "128": "icons/icon3.png"
        }
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "notifications"
    ],
    "commands": {
        "show_prompter": {
            "suggested_key": "Alt+S",
            "description": "Show prompter"
        }
    },
    "background": {
        "service_worker": "script.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "prompter.css"
            ]
        }
    ]
}